<h1>Albums#show</h1>
<p>Find me in app/views/albums/show.html.erb</p>



<table class="table mt-4">
<tr>
    <td>id</td>
    <td>name</td>
    <td>url</td>
    <td>Album</td>
    <td>Artist</td>
    
    <td>actions</td>
</tr>
<% @album.songs.each do |song| %>
<tr>
    <td>
    <%#= link_to song.id, song_path(song.id) %>
    
    </td>
    <td><%= song.name %></td>
    <td><%= song.url %></td>
    <td><%= song.album.name %></td>
    <td>
    <% song.artists.each do |a| %>
            <li> <%= a.name %> </li>
        <% end %>
    </td>
        
    <td>
        <%= link_to "Edit", edit_album_song_path(@album, song), class: "btn btn-warning" %>
    </td>
</tr>

<% end %>

</table>